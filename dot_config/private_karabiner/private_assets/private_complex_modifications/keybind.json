{
	"title": "Disable Cmd + S in Specific App",
	"rules": [
		{
			"description": "Disable Cmd + S in Specific App",
			"manipulators": [
				{
					"from": {
						"key_code": "s",
						"modifiers": {
							"mandatory": [
								"left_command"
							]
						}
					},
					"to": [
						{
							"key_code": "vk_none"
						}
					],
					"conditions": [
						{
							"name": "frontmost_application_if",
							"bundle_identifiers": [
								"com.journey.ios.app/Applications/Journey.app/Contents/MacOS/Journey"
							]
						}
					]
				}
			]
		}
	]
}
